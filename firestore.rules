rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // Cache collections - only accessible by Cloud Functions (admin SDK)
    match /cache_cities/{document=**} {
      allow read, write: if false; // Only admin SDK can access
    }

    match /cache_weather/{document=**} {
      allow read, write: if false; // Only admin SDK can access
    }
  }
}
