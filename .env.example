# Configuration de l'application IWantSun
# Copiez ce fichier vers .env et remplissez avec vos vraies clés API

# Amadeus API - https://developers.amadeus.com/
# Gratuit jusqu'à 2000 requêtes/mois
AMADEUS_API_KEY=your_amadeus_api_key_here
AMADEUS_API_SECRET=your_amadeus_api_secret_here
AMADEUS_API_URL=https://test.api.amadeus.com

# Google Places API - https://console.cloud.google.com/
GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# OpenWeather API (optionnel, Open-Meteo est gratuit)
OPENWEATHER_API_KEY=your_openweather_api_key_here

# Configuration
ENABLE_LOGGING=true
CACHE_DURATION_HOURS=24
API_TIMEOUT_SECONDS=30
